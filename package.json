  "repository": {
    "type": "git",
    "url": "https://github.com/en0c/pixel4te.git"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --check --ignore-path .gitignore --write .",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@headlessui-float/react": "^0.10.1",
    "@headlessui/react": "^1.7.13",
    "@openzeppelin/contracts": "^4.8.2",
    "@wagmi/chains": "^0.2.11",
    "@wagmi/connectors": "^0.3.2",
    "@wagmi/core": "^0.10.1",
    "alchemy-sdk": "2.6.1",
    "axios": "^1.2.1",
    "ethers": "^5",
    "flowbite": "1.6.3",
    "flowbite-react": "0.4.2",
    "mongodb": "5.1.0",
    "next": "13.2.3",
    "nft.storage": "^7.0.0",
    "react": "18",
    "react-dom": "18",
    "react-icons": "^4.4.0",
    "react-share": "^4.4.1",
    "wagmi": "^0.12.1"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "@types/react": "18",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "10.4.8",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "tailwindcss": "^3.1.7",
    "typescript": "4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "ignorePatterns": ["node_modules/", "hardhat/"],
    "extends": [
      "eslint:recommended",
      "next/core-web-vitals",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "plugin:tailwindcss/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "jsx-a11y",
      "prettier",
      "react",
      "tailwindcss"
    ],
    "root": true,
    "rules": {
      "no-undef": "off",
      "react/react-in-jsx-scope": "off",
      "tailwindcss/classnames-order": [
        "warn",
        {
          "officialSorting": true
        }
      ]
    }
  }
}
